<div id="#Accordion_list_<%= member.id %>" data-children=".item">
 <div class="item">
   <span class="badge badge-default">
     <%= member.list.size %>
   </span>
   <a  class='btn btn-sm' data-toggle="collapse" data-parent="#Accordion_list_<%= member.id %>" href="#member_<%= member.id %>" aria-expanded="true" aria-controls="member_<%= member.id %>">
     <%= member.name %>
   </a>
   <div id="member_<%= member.id %>" class="collapse" role="tabpanel">
       <div class="row">
       <div class="col">
       </div>
       <div class="col-11">
         <%= render 'lists/list', member:member %>
       </div>
     </div>
   </div>
 </div>
</div>
