$('#dialog').modal('toggle');
<% @chore.scopes.each do |scope, scope_title| %>
  $("#<%= badge_id(scope, @chore) %>")
      .html("<%= Chore.send(scope).count %>");
  $("#<%= tbody_id(scope, @chore) %>")
      .prepend("<tr class='<%= @chore.row_id %>'></tr>");
  $(".<%= @chore.row_id %>")
      .html("<%= j render "chores/chore", chore:@chore %>");
<% end %>
